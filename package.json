{
  "name": "blood-diamond-ink",
  "version": "1.0.0",
  "private": true,
  "browserslist": {
    "production": ["Chrome >= 120", "last 1 Safari version", "last 1 Firefox version"],
    "development": ["last 1 Chrome version"]
  },
  "scripts": {
    "predev": "node scripts/gen-icons.mjs || true",
    "dev": "next dev",
    "build": "next build",
    "cf:build": "node scripts/gen-icons.mjs && npx @cloudflare/next-on-pages@1 build",
    "cf:preview": "wrangler pages dev .vercel/output/static --compatibility-date=2024-11-11 --local true",
    "postbuild": "node scripts/gen-icons.mjs && next-sitemap",
    "spacing-fix": "node scripts/spacing-fix.cjs",
    "start": "next start",
    "lint": "next lint",
    "diagnose-build": "npm run build && ls -l .next/static/css/"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.5",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "next": "^14.2.33",
    "next-sitemap": "^4.2.3",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "esbuild": "^0.25.11",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "sharp": "^0.33.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "engines": {
    "node": "20"
  }
}
