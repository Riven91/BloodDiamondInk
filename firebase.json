{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.png",
        "headers": [
          { "key": "Content-Type", "value": "image/png" },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/logo-blooddiamond.svg",
        "headers": [{ "key": "Cache-Control", "value": "public, max-age=300" }]
      },
      {
        "source": "/_next/static/**",
        "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
      },
      {
        "source": "/index.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=600, must-revalidate"
          }
        ]
      },
      {
        "source": "/pforzheim/index.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=600, must-revalidate"
          }
        ]
      },
      {
        "source": "/heilbronn/index.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=600, must-revalidate"
          }
        ]
      },
      {
        "source": "/boeblingen/index.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=600, must-revalidate"
          }
        ]
      }
    ],
    "frameworksBackend": {
      "id": "my-web-app",
      "region": "europe-west4"
    },
    "redirects": [
      { "source": "/favicon", "destination": "/favicon.ico", "type": 301 },

      { "source": "/favicon grundlage.png", "destination": "/favicon-16x16.png", "type": 301 },
      { "source": "/favicon%20grundlage.png", "destination": "/favicon-16x16.png", "type": 301 },

      { "source": "/favicon grundlage (1).png", "destination": "/favicon-32x32.png", "type": 301 },
      { "source": "/favicon%20grundlage%20(1).png", "destination": "/favicon-32x32.png", "type": 301 },

      { "source": "/favicon grundlage (2).png", "destination": "/favicon-48x48.png", "type": 301 },
      { "source": "/favicon%20grundlage%20(2).png", "destination": "/favicon-48x48.png", "type": 301 },

      { "source": "/favicon grundlage (3).png", "destination": "/apple-touch-icon.png", "type": 301 },
      { "source": "/favicon%20grundlage%20(3).png", "destination": "/apple-touch-icon.png", "type": 301 },

      { "source": "/favicon grundlage (4).png", "destination": "/favicon-96x96.png", "type": 301 },
      { "source": "/favicon%20grundlage%20(4).png", "destination": "/favicon-96x96.png", "type": 301 },

      { "source": "/favicon grundlage (5).png", "destination": "/favicon-128x128.png", "type": 301 },
      { "source": "/favicon%20grundlage%20(5).png", "destination": "/favicon-128x128.png", "type": 301 },

      { "source": "/favicon grundlage2.png", "destination": "/og-image.png", "type": 301 },
      { "source": "/favicon%20grundlage2.png", "destination": "/og-image.png", "type": 301 }
    ]
  }
}
